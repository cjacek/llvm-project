RUN: llvm-mc -filetype=obj -triple=aarch64-windows test2-arm64.s -o test2-arm64.obj
RUN: llvm-mc -filetype=obj -triple=arm64ec-windows test3-arm64ec.s -o test3-arm64ec.obj
